[push]
  default = current
[alias]
  co = checkout
[credential]
  helper = osxkeychain
[color]
  # Use colors in Git commands that are capable of colored output when outputting to the terminal
  ui     = auto
  diff   = auto
  status = auto
  branch = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = green
  changed = red
  untracked = cyan
[core]
  excludesfile = ~/.gitignore
  autocrlf     = input
  editor       = vim
[user]
  name = Dan Corrigan
	email = daniel.corrigan@voxmedia.com
[blame]
	ignoreRevsFile = .git-blame-ignore-revs
